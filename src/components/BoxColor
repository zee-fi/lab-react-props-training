

function BoxColor(props) {
    const divStyle = {
        backgroundColor: `rgb(${props.red}, ${props.green}, ${props.blue})`,
        width: '100px',
        height: '100px',
        border: '1px solid black',
        padding: '10px',
        margin: '10px',
    };
    return (
        <div style={divStyle}>
            <p>rgb({props.red}, {props.green}, {props.blue})</p> {/* Display the RGB values */}
        </div>
    );
}


export default BoxColor; 